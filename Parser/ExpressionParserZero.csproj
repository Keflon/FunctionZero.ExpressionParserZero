<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageId>FunctionZero.ExpressionParserZero</PackageId>
    <Version>3.1.0</Version>
    <Description>RPN Expression Parser and Evaluator</Description>
    <Copyright>Copyright 2017-2021 (c) Function Zero Ltd.</Copyright>
    <PackageTags>RPN postfix expression parser evaluator infix reverse polish overload typesafe variable variables</PackageTags>
    <PackageReleaseNotes>Changes to backing store, allowing greater flexibility.</PackageReleaseNotes>
    <Authors>Keith Pickford</Authors>
    <Company>Function Zero Ltd</Company>
    <RootNamespace>FunctionZero.ExpressionParserZero</RootNamespace>
    <PackageProjectUrl>https://github.com/Keflon/FunctionZero.ExpressionParserZero</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Keflon/FunctionZero.ExpressionParserZero</RepositoryUrl>
    <PackageLicenseExpression></PackageLicenseExpression>
    <PackageLicenseFile>License.txt</PackageLicenseFile>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\License.txt">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <Folder Include="FunctionMatrices\Generated\" />
    <Folder Include="FunctionVectors\Generated\" />
  </ItemGroup>

  <Target Name="PreBuild" BeforeTargets="PreBuildEvent">
    <Exec Command="..\MatrixCodeGen\bin\Debug\netcoreapp3.0\MatrixCodeGen UnaryComplementVector ~ &gt; $(ProjectDir)\FunctionVectors\Generated\UnaryComplementVector.cs&#xD;&#xA;&#xD;&#xA;rem ..\MatrixCodeGen\bin\Debug\netcoreapp3.0\MatrixCodeGen LogicalAndMatrix &quot;&amp;&quot; &gt; $(ProjectDir)\FunctionMatrices\Generated\LogicalAndMatrix.cs&#xD;&#xA;rem ..\MatrixCodeGen\bin\Debug\netcoreapp3.0\MatrixCodeGen AddMatrix + &gt; $(ProjectDir)\FunctionMatrices\Generated\AddMatrix.cs&#xD;&#xA;rem ..\MatrixCodeGen\bin\Debug\netcoreapp3.0\MatrixCodeGen MultiplyMatrix * &gt; $(ProjectDir)\FunctionMatrices\Generated\MultiplyMatrix.cs&#xD;&#xA;rem ..\MatrixCodeGen\bin\Debug\netcoreapp3.0\MatrixCodeGen SubtractMatrix - &gt; $(ProjectDir)\FunctionMatrices\Generated\SubtractMatrix.cs&#xD;&#xA;" />
  </Target>

</Project>
