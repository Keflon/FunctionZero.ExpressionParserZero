<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageId>FunctionZero.ExpressionParserZero</PackageId>
    <Version>4.0.0</Version>
    <Description>RPN Expression Parser and Evaluator with databinding to POCO and INPC instances.</Description>
    <Copyright>Copyright 2017-2021 (c) Function Zero Ltd.</Copyright>
    <PackageTags>RPN postfix expression parser evaluator infix reverse polish overload typesafe variable variables bind binding zbind z:bind cast casting</PackageTags>
    <PackageReleaseNotes>DataBinding support! Bind directly to POCO/INPC properties. All csharp value-types supported. Typecasting supported. Full support for null and all nullable types.</PackageReleaseNotes>
    <Authors>Keith Pickford</Authors>
    <Company>Function Zero Ltd</Company>
    <RootNamespace>FunctionZero.ExpressionParserZero</RootNamespace>
    <PackageProjectUrl>https://github.com/Keflon/FunctionZero.ExpressionParserZero</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Keflon/FunctionZero.ExpressionParserZero</RepositoryUrl>
    <PackageLicenseExpression></PackageLicenseExpression>
    <PackageLicenseFile>License.txt</PackageLicenseFile>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\License.txt">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <Folder Include="FunctionMatrices\Generated\FunctionMatrices\" />
  </ItemGroup>

</Project>
